    "button-group": {
        "description": "",
        "displayName": "slds-button-group",
        "props": {
            "children": {
                "type": {
                    "name": "custom",
                    "raw": "function (props, propName, componentName) {\n    const prop = props[propName];\n    let error;\n    React.Children.forEach(prop, child => {\n        if (!error && ( child.type !== Button && child.type !== Dropdown ) ) {\n            error = new Error(\n                '`' + componentName + '` ' +\n                'should only contain children of the type `Button` or `Dropdown`.'\n            );\n        }\n    });\n\n    if (error) {\n        return error;\n    }\n}"
                },
                "required": false,
                "description": ""
            }
        }
    },
